.home {
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
}
.home image {
  z-index: 1;
  width: 100%;
  height: 100%;
  position: fixed;
}
.wrapper {
  width: 600rpx;
  height: 400rpx;
  display: flex;
  align-self: flex-start;
  margin-top: 50rpx;
  flex-direction: column;
  justify-content: space-between;
  z-index: 2;
}
.row {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 600rpx;
}
.row  button {
  width: 250rpx;
  height: 150rpx;
  line-height: 150rpx;
  text-align: center;
  border: 4rpx black solid;
  box-shadow: black 0 0 5rpx 2rpx;
  background-color: rgba(0, 0, 0, 0);
}